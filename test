 pipeline {

  agent { kubernetes {
    label 'kubernetes.io/hostname: node1'
      containerTemplate {
        name 'jnlp'
        image 'jenkinsci/jnlp-slave:2.62-alpine'
        ttyEnabled true
        args '${computer.jnlpmac} ${computer.name}'
      }
  }}


  stages {
    stage('Run maven') {
      steps {
        sh 'hostname'
      }
    }
  }
}
